            .cellpopup .cellpopuptext::after {
                content: "";
                visibility: hidden;
                position: absolute;
                top: 100%;
                left: 50%;
                margin-left: -5px;
                border-width: 5px;
                border-style: solid;
                border-color: rgb(10, 89, 248) transparent transparent transparent;
            }
